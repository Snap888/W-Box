unzip_file() {
    local file_path="$1"
    local dest_dir="$2"
    if [ -e "$file_path" ]; then
        echo "Ð Ð°ÑÐ¿Ð°ÐºÐ¾Ð²ÐºÐ° $file_path Ð² $dest_dir..."
        unzip -o "$file_path" -d "$dest_dir"
        echo "Ð£ÑÐ¿ÐµÑˆÐ½Ð¾ Ñ€Ð°ÑÐ¿Ð°ÐºÐ¾Ð²Ð°Ð½Ð¾."
    else
        echo "Ð¤Ð°Ð¹Ð» $file_path Ð½Ðµ Ð½Ð°Ð¹Ð´ÐµÐ½."
    fi
}
