	taskset -c $PRIM_COR_86 $GLIBC_BIN/box64 $GLIBC_BIN/wine64 regedit $PREFIX/glibc/opt/prefix/system.reg &>/dev/null
	if [ ! "$STARTUP_COMPATIBILITY_MODE" = "1" ]; then
