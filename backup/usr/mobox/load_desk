			DISPLAY=:0 taskset -c $PRIM_COR_86 $GLIBC_BIN/box64 $GLIBC_BIN/wine64 explorer /desktop=shell,640x480 cmd /c "start Z:\\usr\\glibc\\opt\\apps\\install.bat" >${LOG_PATH}_redist.txt 2>&1
		if [ -f "$DXVK_FILE_86" ]; then
	    	sleep 1
