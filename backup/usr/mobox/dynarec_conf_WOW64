sed -i "64s/.*/$ATOMICS_WOW_64/" "$CONFIG"
sed -i "65s/.*/$BIGBLOCK_WOW_64/" "$CONFIG"
sed -i "66s/.*/$FASTNAN_WOW_64/" "$CONFIG"
sed -i "67s/.*/$SAFEFLAGS_WOW_64/" "$CONFIG"
sed -i "68s/.*/$STRONGMEM_WOW_64/" "$CONFIG"
