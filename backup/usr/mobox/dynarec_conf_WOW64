        IGNOREINT3_WOW_64="echo 'unset BOX64_IGNOREINT3'"
echo "unset BOX64_IGNOREINT3">$PREFIX/glibc/opt/conf/dynarec/ignoreint3.conf
    elif [ $FIRST_CHAR = "2" ]; then
        ATOMICS_WOW_0="ATOMICS_WOW=-"
        ATOMICS_WOW="-"
